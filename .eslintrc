{
  "extends": "@antfu",
  "rules": {
    "quotes": "off",
    "@typescript-eslint/quotes": ["error", "double", { "avoidEscape": true }],
    "indent": "off",
    "@typescript-eslint/indent": ["error", 4, { "SwitchCase": 1, "VariableDeclarator": 1, "outerIIFEBody": 1 }],
    "vue/html-indent": ["error", 4]
}
}
